<footer class="atlm-footer">
  {{!-- DESKTOP FOOTER --}}
  <div
    class="uk-padding-remove uk-margin-remove uk-flex-center uk-flex-middle uk-text-center uk-grid-collapse uk-padding uk-child-width-1-1@s uk-child-width-1-3@m  uk-visible@l"
    uk-grid uk-height-match target="> section > .uk-card" row="false">
    {{#each this.partners as |partner|}}
      <div class="uk-padding-small">
        <a href={{partner.link}}>
          <img src={{partner.image}} alt={{concat partner.name " Branding"}} />
        </a>
      </div>
    {{/each}}
    <div class="uk-margin-small-top uk-width-1-1 uk-clearfix atlm-sub-footer">

      <div class="uk-float-left uk-padding-small">
        <img class="atlm-small-logo" src="/assets/images/atl-bw.png" alt="" role="presentation" /> <a
          href="http://digitalscholarship.emory.edu/about/center/privacy-policy.html">Terms and Privacy</a>
      </div>
      <div class="uk-float-right uk-padding-small">
        <a href="https://github.com/ecds/ATLMaps" aria-label="ATLMaps on GitHub">
          <FaIcon @prefix="fab" @icon="github" />
        </a>
      </div>
    </div>
  </div>
  {{!-- SMALL SCREEN FOOTER --}}
  <div class=" uk-hidden@l">
    Footer
  </div>
</footer>