<ul class="uk-thumbnav atlm-base-map-selector" uk-margin>
  {{#each this.baseMaps.baseMaps as |base|}}
    <li class="uk-box-shadow-xlarge {{if (eq base.label @project.defaultBaseMap) "active"}}" id={{concat "base-" base.label}}>
      <UkButton @color="link" {{on 'click' (fn this.switchBase base)}}>
        <img src={{base.thumbnail}} width="150" alt={{base.label}}>
      </UkButton>
    </li>
  {{/each}}
</ul>