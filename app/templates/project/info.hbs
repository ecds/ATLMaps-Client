{{#project-pane title='About this Project'}}
{{#md-card title="Info" id="basic-card"}}
    {{#md-card-content}}
        {{model.description}}
    {{/md-card-content}}
    {{#md-card-action}}
      {{#if model.hasIntro}}
        {{md-btn text='Show Intro'
      	 action='toggleIntro'
      	  class='light-blue darken-2'}}
        {{/if}}
    {{/md-card-action}}
{{/md-card}}
{{/project-pane}}
