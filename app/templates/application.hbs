{{outlet 'modal'}}
<div class="content">

    <div class="navbar main-navbar">
    <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-inverse-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
        </button>
        {{#link-to 'index' tagName='a' class='site-title navbar-brand'}}
          <img class="logo" src="/assets/images/am-logo3.png"/><span class="alt-dark">ATL</span><span>Maps</span>
        {{/link-to}}
    </div>
